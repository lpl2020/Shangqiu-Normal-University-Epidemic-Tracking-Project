<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.demo.sq.mappers.VersionMapper" >
  <resultMap id="BaseResultMap" type="com.example.demo.sq.entity.Version" >
    <result column="sys_version" property="sysVersion" jdbcType="VARCHAR" />
    <result column="mysql_version" property="mysqlVersion" jdbcType="VARCHAR" />
  </resultMap>
  <insert id="insert" parameterType="com.example.demo.sq.entity.Version" >
    insert into version (sys_version, mysql_version)
    values (#{sysVersion,jdbcType=VARCHAR}, #{mysqlVersion,jdbcType=VARCHAR})
  </insert>
  <insert id="insertSelective" parameterType="com.example.demo.sq.entity.Version" >
    insert into version
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="sysVersion != null" >
        sys_version,
      </if>
      <if test="mysqlVersion != null" >
        mysql_version,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="sysVersion != null" >
        #{sysVersion,jdbcType=VARCHAR},
      </if>
      <if test="mysqlVersion != null" >
        #{mysqlVersion,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
</mapper>