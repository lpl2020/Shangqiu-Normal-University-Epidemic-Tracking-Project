<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.demo.sq.mappers.SetupTimersMapper" >
  <resultMap id="BaseResultMap" type="com.example.demo.sq.entity.SetupTimers" >
    <result column="NAME" property="name" jdbcType="VARCHAR" />
    <result column="TIMER_NAME" property="timerName" jdbcType="CHAR" />
  </resultMap>
  <insert id="insert" parameterType="com.example.demo.sq.entity.SetupTimers" >
    insert into setup_timers (NAME, TIMER_NAME)
    values (#{name,jdbcType=VARCHAR}, #{timerName,jdbcType=CHAR})
  </insert>
  <insert id="insertSelective" parameterType="com.example.demo.sq.entity.SetupTimers" >
    insert into setup_timers
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="name != null" >
        NAME,
      </if>
      <if test="timerName != null" >
        TIMER_NAME,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="name != null" >
        #{name,jdbcType=VARCHAR},
      </if>
      <if test="timerName != null" >
        #{timerName,jdbcType=CHAR},
      </if>
    </trim>
  </insert>
</mapper>